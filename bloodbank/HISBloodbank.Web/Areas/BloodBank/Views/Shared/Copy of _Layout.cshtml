<body>

    <div data-backdrop="static" data-keyboard="false" class="modal small" id="FrmDeptInfo" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">

                    <div class="row">
                        <div class="col-xs-5">                        
                            <h4 class="modal-title" id="myModalLabel" style="color:black;">DEPARTMENT INFO.</h4>
                        </div>                        
                    </div> 

                </div>
                <div class="modal-body form-horizontal">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-xs-3">
                                <label class="control-label">Division</label>
                            </div>
                            <div class="col-xs-9">
                                <input id="select2Division101" name="" type="text" class="form-control">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-3">
                                <label class="control-label">Department</label>
                            </div>
                            <div class="col-xs-9">
                                <input id="select2Department101" name="" type="text" class="form-control">
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-3">
                                <label class="control-label">Station</label>
                            </div>
                            <div class="col-xs-9">
                                <input id="select2Station101" name="" type="text" class="form-control">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">

                    <div class="pull-right">

                        <a onclick="SetUpClose();" id="btnCloseDeptInfo" class="btn btn-sm btn-danger pull-right pull-right-margin ShowOnAdd ShowOnView ShowOnEdit">
                            <span class="glyphicon glyphicon-remove"></span> Close
                        </a>                
                        <a onclick="SetUpSave();" id="btnSaveDeptInfo" class="btn btn-sm btn-success pull-right pull-right-margin ShowOnAdd HideOnView ShowOnEdit">
                            <span class="glyphicon glyphicon-floppy-disk"></span> Save
                        </a>

                    </div>

                </div>
            </div>
        </div>
    </div>

    
    <nav id="topnavbar" class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="application-title">
            <div id="header-caption" class="pull-left cAR-title"> <b style="color:red;">TEST SERVER</b>   SGH Group - BloodBank</div>
            @*<div class="pull-right cAR-uname"><span class="glyphicon glyphicon-user"></span> Welcome: <b>@User.Identity.Name!</b></div>*@

              <div class="pull-left cAR-title"id="txtstationame" 
                data-urlsecure="@Url.Action("SecurityFeature", "Base", new { area = "" })" 
                data-urlfunction="@Url.Action("SecurityFunction", "Base", new { area = "" })"
                data-appmenu="@Url.Action("GetMenu", "Home", new { area = "" })"
                >
                @ViewBag.Title                 
            </div>

            <div class="pull-right AR_user_cont">
                <ul class="nav navbar-nav ar_mini_top_menu" style="margin-right:5px !important;">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"> 
                            <span class="glyphicon glyphicon-cog"></span>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li>
                                <a href="@Url.Action("Logoff", "Home", new { area = "BloodBank" })" data-toggle="tooltip" title="Click here to logout...">
                                    <span class="glyphicon glyphicon-log-out"></span> Logout
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
                <user><span class="glyphicon glyphicon-user"></span> User: @User.Identity.Name!</user>
            </div>

        </div>
 

          <div class="clear-both"></div>
        <div class="container-fluid">
            <div class="navbar-header">
                <a style="background-color:#D2DE96!important; color:black!important;" class="navbar-brand" href="/HISLOGIN"><span class="glyphicon glyphicon-home"></span></a>                
                   
            </div>
            <div class="navbar-collapse collapse" id="bs-example-navbar-collapse-1">
                <div id="url-menu"></div>
                 <div class="pull-right">
                    <select style="margin-top:4px;" id="ListOfStation"></select>
                </div>
            </div>
             
        </div>

    </nav>


    <div id="container" class="container-fluid cAR-panel-top cAR-margin-top his-base-url _mainiframe" data-baseurl="@Url.Action("", new { area = "BloodBank" })" data-baseurl2 = "@Url.Action("")">       
        @RenderBody()
    </div>


    <!-- Preloader -->
    <div id="preloader">
        <div id="status">&nbsp;
            <div id="spinningSquaresG">
                <div id="spinningSquaresG_1" class="spinningSquaresG">
                </div>
                <div id="spinningSquaresG_2" class="spinningSquaresG">
                </div>
                <div id="spinningSquaresG_3" class="spinningSquaresG">
                </div>
                <div id="spinningSquaresG_4" class="spinningSquaresG">
                </div>
                <div id="spinningSquaresG_5" class="spinningSquaresG">
                </div>
                <div id="spinningSquaresG_6" class="spinningSquaresG">
                </div>
                <div id="spinningSquaresG_7" class="spinningSquaresG">
                </div>
                <div id="spinningSquaresG_8" class="spinningSquaresG">
                </div>
            </div>
        </div>
    </div>
    
    <div id="statusbar" class="form-horizontal" style="position:fixed; bottom:0; background-color:#1BA39C; width:100%; height:25px;z-index:1000; ">
        <div class="col-xs-3">
            <label id="svrinfo" class="control-label" style="color:white;" ></label>
        </div>
    </div>

    @RenderSection("scripts", required: false)
    @RenderSection("styles", required: false)

</body>
