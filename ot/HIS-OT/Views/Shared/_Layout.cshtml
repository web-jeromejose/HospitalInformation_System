@using System.Web.Optimization;

@{
    ViewBag.Title = "Operation Theater";
    var version = 2019.02;
    // update this version everytime you change js file for the browser to reload the script JFJ feb 2019 📈
}
<!DOCTYPE html>
<html lang="en">

<head>
    <title>🏥 OT WEB Module</title>
    <link rel="shortcut icon" href="../favicon.ico" />
    <link href="~/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/jqueryui/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/jqueryui/jquery-ui.theme.min.css" rel="stylesheet" />
    <link href="~/Content/datatable/css/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/datatable/css/jquery.dataTables_themeroller.css" rel="stylesheet" />
    <link href="~/Content/datatable/css/dataTables.responsive.css" rel="stylesheet" />
    <link href="~/Content/datetimepicker/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <link href="~/Content/icheck/skins/all.css" rel="stylesheet" />

    <link rel="stylesheet" href="@Url.Content("~/Content/dist/css/AdminLTE.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/dist/bower_components/font-awesome/css/font-awesome.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/dist/bower_components/Ionicons/css/ionicons.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/dist/plugins/iCheck/all.css")">
    <link href="~/Content/sweetalert/sweet-alert.css" rel="stylesheet" />


    <link href="~/Content/dhtmlxscheduler/dhtmlxscheduler.css" rel="stylesheet" />
    @*<link href="~/Content/yamm/yamm.css" rel="stylesheet" />*@
    <link href="~/Content/imported/css/fahad.css" rel="stylesheet" />
    <link href="~/Content/codebase/dhtmlx.css" rel="stylesheet" />
    <link href="~/Content/toastr/toastr.min.css" rel="stylesheet" />
    <link href="~/Content/select2/select2.css" rel="stylesheet" />
    <link href="~/Content/select2/select2-bootstrap.css" rel="stylesheet" />
    <link href="~/Content/lcswitch/css/lc_switch.css" rel="stylesheet" />


    <link href="~/Content/your_style.css" rel="stylesheet" />


    @*javascript*@
    <script src="~/Content/jquery/js/jquery.min.js"></script>
    <script src="@Url.Content("~/Content/common/js/common.js?ver="+@version.ToString())"></script>
    <script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Content/moment/js/moment.js"></script>
    <script src="~/Content/datejs/date.js"></script>
    <script src="~/Content/accounting/js/accounting.js"></script>
    <script src="~/Content/bootbox/js/bootbox.min.js"></script>
    <script src="~/Content/datatable/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/datatable/extensions/RowGrouping/jquery.dataTables.grouping.js"></script>
    <script src="~/Content/datatable/extensions/Editable/js/jquery.dataTables.editable.js"></script>
    <script src="~/Content/datatable/extensions/Editable/js/jquery.jeditable.js"></script>
    <script src="~/Content/datetimepicker/js/bootstrap-datetimepicker.min.js"></script>
    <script src="~/Content/icheck/icheck.min.js"></script>
    <script src="~/Content/dhtmlxscheduler/dhtmlxscheduler.js"></script>
    <script src="~/Content/dhtmlxscheduler/ext/dhtmlxscheduler_tooltip.js"></script>
    <script src="~/Content/codebase/dhtmlx.js"></script>
    <script src="~/Content/toastr/toastr.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    <script src="~/Content/select2/select2.js"></script>
    <script src="~/Content/lcswitch/js/lc_switch.js"></script>
    <script src=" @Url.Content("~/Scripts/ajaxWrapper.js?ver="+@version.ToString())"></script>
    <script src="@Url.Content("~/Scripts/Index.js?ver="+@version.ToString())"></script>
    <script src="@Url.Content("~/Scripts/jqSecurity.js?ver="+@version.ToString())"></script>
    <script src="~/Scripts/Global/hisglobalsettings.js"></script>

    <script src="~/Content/sweetalert/sweet-alert.min.js"></script>

    <script src="@Url.Content("~/Content/dist/js/adminlte.js")"></script>
    <script src="@Url.Content("~/Content/dist/js/demo.js")"></script>
    <script src="@Url.Content("~/Content/dist/jquery.hotkeys-master/jquery.hotkeys.js")"></script>
    <script defer src="@Url.Content("~/Scripts/DataTabletoPdf.js")" type="text/javascript"></script>

</head>
<body>


    <nav id="topnavbar" class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="application-title">
            <div id="header-caption" class="pull-left cAR-title" data-urlsecure="@Url.Action("SecurityFeature", "Base", new { area = "" })">SGH Group - Operation Theater</div>
            <div class="pull-left cAR-title">
            </div>
            <div class="pull-right cAR-uname"><span class="glyphicon glyphicon-user"></span> Welcome: <b>@User.Identity.Name!</b></div>
        </div>
        <div class="clear-both"></div>
        <div class="container-fluid">
            <div class="navbar-header">
                @if (Request.Url.LocalPath == "/HISOT/OT/Home")
                {
                    <a style="background-color:#D2DE96!important; color:black!important;" class="navbar-brand" href="/HISLOGIN"><span class="glyphicon glyphicon-home"></span></a>

                }
                else
                {
                    <a style="background-color:#D2DE96!important; color:black!important;" class="navbar-brand" href="@Url.Action("Index", "Home", new { area = "" })"><span class="glyphicon glyphicon-home"></span></a>

                }

            </div>
            <div class="navbar-collapse collapse" id="bs-example-navbar-collapse-1">

                <div id="url-menu"></div>

                <div class="pull-right">
                    <select style="margin-top:4px;" id="ListOfStation" class="form-control-sm select2 RequiredField "></select>
                </div>
            </div>
        </div>
    </nav>


    <div id="container" class="container-fluid cAR-panel-top cAR-margin-top his-base-url _mainiframe" data-baseurl="@Url.Action("", new { area = "OT" })" data-baseurl2="@Url.Action("")">
        <input name="password" disabled="disabled" type="password" style="display:none;">
        <input name="password" disabled="disabled" type="search" style="display:none;">

        @RenderBody()
    </div>


    <!-- Preloader -->
    <div id="preloader">
        <div id="status">&nbsp;</div>
    </div>


    <div class="pull-left cAR-title" id="txtstationame"
         data-urlsecure="@Url.Action("SecurityFeature", "Base", new { area = "" })"
         data-urlfunction="@Url.Action("SecurityFunction", "Base", new { area = "" })"
         data-appmenu="@Url.Action("GetMenu", "Home", new { area = "" })"
         data-appissue="@Url.Action("GetApplicationIssue", "Home", new { area = "" })"
         data-getlistofstation="@Url.Action("GetListOfStation", "Home")"
         data-setcurrentstation="@Url.Action("SetCurrentStation", "Home")">

    </div>


    @RenderSection("scripts", required: false)
    @RenderSection("styles", required: false)

</body>
</html>









