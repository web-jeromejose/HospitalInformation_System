
<div class="modal  " id="modal-showselected" style=" padding-right: 17px;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title">Default Modal</h4>
            </div>
            <div class="modal-body">
                <div class="box box-success">
                    <div class="box-header">
                        <div class="col-sm-12">
                            <div class="box-tools pull-right">
                                <button id="btn-save" class="btn btn-success btn-sm"><span aria-hidden="true" class="glyphicon glyphicon-ok"></span>Save</button>
                                <button id="btn-can" class="btn btn-danger btn-sm"><span aria-hidden="true" class="glyphicon glyphicon-remove"></span>Cancel</button>
                                <a class="btn btn-default btn-sm" href="@Url.Action("Index", "NursingAdministration")">Back</a>
                            </div>
                        </div>
                    </div>
                    <div class="box-body">
                        <div class="form-horizontal">
                            <div class="form-group">
                                <div class="col-sm-12">
                                    <label for="txtpatient" class="col-sm-1 control-label">PIN</label>
                                    <div class="col-sm-2">
                                        <input type="text" id="txtpatient" class="sel" tabindex="1" data-orderid="0" data-presid="0" data-pprid="0" data-drid="0" />
                                    </div>
                                    <label for="txtbed" class="col-sm-1 control-label">Bed No</label>
                                    <div class="col-sm-2">
                                        <input type="text" id="txtbed" class="form-control" readonly />
                                    </div>
                                    <label for="prefixorderno" class="col-sm-1 control-label">Sl. No</label>
                                    <div class="col-sm-2">
                                        <input id="prefixorderno" type="text" class="form-control" readonly />
                                    </div>
                                    <label for="date" class="col-sm-1 control-label">
                                        Date
                                    </label>
                                    <div class="col-sm-2">
                                        <input id="dt" type="text" class="form-control" readonly value="@DateTime.Now.ToString("dd-MMM-yyyy HH:mm")" />
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <label for="txtname" class="col-sm-1 control-label">
                                        Name
                                    </label>
                                    <div class="col-sm-5">
                                        <input id="txtname" type="text" class="form-control" readonly />
                                    </div>
                                    <label for="ward" class="col-sm-1 control-label">
                                        Ward
                                    </label>
                                    <div class="col-sm-2">
                                        <input id="ward" type="text" class="form-control" style="font-size: 10px;" readonly />
                                    </div>
                                    <label for="package" class="col-sm-1 control-label">
                                        Package
                                    </label>
                                    <div class="col-sm-2">
                                        <input id="package" type="text" class="form-control" readonly />
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <label for="txtage" class="col-sm-1 control-label">
                                        Age
                                    </label>
                                    <div class="col-sm-2">
                                        <input id="txtage" type="text" class="form-control" readonly />
                                    </div>
                                    <label for="txtsex" class="col-sm-1 control-label">
                                        Sex
                                    </label>
                                    <div class="col-sm-2">
                                        <input id="txtsex" type="text" class="form-control" readonly />
                                    </div>

                                    <label for="txtcompany" class="col-sm-1 control-label">
                                        Company
                                    </label>
                                    <div class="col-sm-5">
                                        <input id="txtcompany" type="text" class="form-control" style="font-size: 10px;" readonly />
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <label for="drugallergies" class="col-sm-1 control-label">
                                        Allergies
                                    </label>
                                    <div class="col-sm-2">
                                        <input id="drugallergies" type="text" class="form-control" readonly />
                                    </div>
                                    <label for="blood" class="col-sm-1 control-label">
                                        Blood
                                    </label>
                                    <div class="col-sm-2">
                                        <input id="blood" type="text" class="form-control" readonly />
                                    </div>

                                    <label for="txtdoctor" class="col-sm-1 control-label">
                                        Doctor
                                    </label>
                                    <div class="col-sm-5">
                                        <input id="txtdoctor" type="text" class="form-control" data-id="0" readonly />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-12">
                            <div class="col-sm-4">
                                <div class="form-horizontal">
                                    <div class="form-group">
                                        <label for="txtpatientID" class="col-sm-2 control-label">Search</label>
                                        <div class="col-sm-10">
                                            <input type="text" id="txtsearchproc" class="sel _select_item" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-sm-8 table-responsive">
                                <table id="tbl-selected" class="table table-condensed">
                                    <thead>
                                        <tr>
                                            <th style="width: 50px;">ID</th>
                                            <th>Procedure</th>
                                            <th style="width: 100px;">Quantity</th>
                                            <th style="width: 50px;"></th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                @*<button type="button" class="btn btn-default pull-left" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Save changes</button>*@
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>



