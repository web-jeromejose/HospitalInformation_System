@{
    ViewBag.Title = "Index";
}

<input type="hidden" id="url"
data-pdf="@Url.Action("ToPDF")"
data-xls="@Url.Action("ToXLS")"
data-getnursingadminis="@Url.Action("NursAdmnProcedboard")"
data-getdeparlist="@Url.Action("DepartmentList")"
data-getnurseview="@Url.Action("FetchNursingAdminis")"
data-save="@Url.Action("Save")"

@*data-getdefaultmarkup="@Url.Action("GetDefaultMarkUp")"*@
/>

<div class="panel panel-default">
   <div class="panel-heading">
      <div class="row">
         <div class="col-sm-3">
            <h4><b class="navbar-header">Nursing Administration Procedure</b></h4>
         </div>
                  <div class="form-group col-sm-9">
                <button id="btnNew" type="button" class="btn btn-sm btn-primary AR_custom_btn_fixed_width glyphicon glyphicon-file  pull-right"> New</button>
            </div>
      </div>
   </div>
           <div class="panel-body">
             <div class="col-sm-12">
                <table id="gridNursingAdmins" class="table table-bordered table-hover dataTable no-footer;">
                    <thead class="AR_custom_table_header expanded-group">
                    <tr>
                    @*<th class="centertext compressedtableheader" style="text-align:center"><input id="iChkAllTest" type="checkbox" value="false"/></th>*@
                    <th>SNo</th>
                    <th>Name</th>
                    <th>Status</th>
                    </tr>
                    </thead>
                </table>
           
            </div>  
        </div>

    
<div class="modal" id="modalEntry" tabindex="-1" style="overflow-y: auto;" role="dialog" aria-labelledby="myModalLabel">
   <input id="Id" name="" type="hidden" class="form-control">
  
   <div class="modal-dialog modal-lg" style=" " role="document">
      <div class="modal-content">
         <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h3 class="modal-title" id="opbmyModalLabel">Nursing Administration Procedures</h3>
         </div>
         <div class="modal-body">
            <div class="row">
               <div class="col-xs-12">
                  <form role="form" class="form-horizontal AR_custom_label_left_align">
                     <div class="form-group">
                          <label class="control-label col-sm-3">Department:</label>
                        <div class="col-sm-7">
                           <input type="text" class="sel" id="Select2Department"/>
                        </div>
                      </div>
                     <div class="form-group">
                             <label class="control-label col-sm-3">Procedure Name:</label>
                        <div class="col-sm-8">
                           <input type="text" class="form-control" id="txtName"/>
                        </div>
                     </div>
                     <div class="form-group">
                        <label class="control-label col-sm-3">Code:</label>
                         <div class="col-sm-5">
                           <input type="text" class="form-control" id="txtCode"/>
                        </div>
                         
                     </div>
                      <div class="form-group">
                          <label class="control-label col-sm-3">Cost Price:</label>
                        <div class="col-sm-3">
                           <input type="number" class="form-control" id="txtCostPrice"/>
                        </div>
                      </div>
                
                     @* 
                     <div id="endos"></div>
                     *@
                  </form>
               </div>
            </div>
            <div class="modal-footer">
             <button id="btnDelete" type="button" class="btn btn-sm btn-danger AR_custom_btn_fixed_width glyphicon glyphicon-trash HideOnAdd ShowOnEdit"> Delete</button>
@*               <button id="btnSave" type="button" class="btn btn-sm btn-success AR_custom_btn_fixed_width glyphicon glyphicon-floppy-disk ShowOnAdd ShowOnView HideOnEdit">Save</button>*@
             <button id="btnPrint" type="button" class="btn btn-sm btn-primary AR_custom_btn_fixed_width glyphicon glyphicon-print ShowOnEdit HideOnAdd" data-dismiss="modal"> Print</button>
             <button id="btnSave" type="button" class="btn btn-sm btn-success AR_custom_btn_fixed_width glyphicon glyphicon-floppy-disk HideOnAdd ShowOnEdit">Save</button>
                
               <button type="button" class="btn btn-sm btn-warning AR_custom_btn_fixed_width" data-dismiss="modal">Cancel</button>
            </div>

         </div>
      </div>
   </div>
</div>
</div>

@section scripts{
<script src="~/Scripts/itadmin/NursAdmnPrced.js"></script>
}