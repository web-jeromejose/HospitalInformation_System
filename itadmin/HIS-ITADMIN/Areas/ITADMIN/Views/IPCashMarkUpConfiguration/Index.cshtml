@{
    ViewBag.Title = "IpCashMarkupConfiguration";
}
<input type="hidden" id="url"
data-getipcashmarkup="@Url.Action("IPCashMarkUpDashboard")"
data-save="@Url.Action("Save")"
data-getdefaultmarkup="@Url.Action("GetDefaultMarkUp")"
/>

<div class="panel panel-default">
   <div class="panel-heading">
      <div class="row">
         <div class="col-sm-3">
            <h4><b class="navbar-header">IP Cash MarkUp Configuration</b></h4>
         </div>
      </div>
   </div>
       <div class="panel-body">

             <div class="col-sm-12">
                <table id="gridIPCashConfig" class="table table-bordered table-hover dataTable no-footer;">
                    <thead class="AR_custom_table_header expanded-group">
                    <tr>
                      <tr>
                        <th colspan="3" class="centertext compressedtableheader" style="text-align:center">Billable Services</th>
                    </tr>
                    @*<th class="centertext compressedtableheader" style="text-align:center"><input id="iChkAllTest" type="checkbox" value="false"/></th>*@
                     <th></th>
                     <th class="centertext compressedtableheader" style="text-align:center">Bed Type</th>
                    <th></th>
                   
                    </tr>
                    </thead>
                </table>
                <div class="pull-right">
                <button id="btnSave" type="button" class="btn btn-sm btn-primary AR_custom_btn_fixed_width glyphicon glyphicon-floppy-disk ShowOnAdd ShowOnView HideOnEdit" data-dismiss="modal">Save</button>
                @* <button id="btnCancelran" type="button" class="btn btn-sm btn-primary AR_custom_btn_fixed_width glyphicon glyphicon-remove ShowOnEdit HideOnAdd" data-dismiss="modal">Cancel</button>*@
                </div>
            </div>  
             <div class="form-group">

                    <label class="control-label col-sm-2">Default Cash MarkUp:</label>
                    <div class="col-sm-1">
                        <input type="number" class="form-control" id="txtMarkUp"/>
                    </div>
             
             </div>

        </div>
   </div>
@section scripts{
<script src="~/Scripts/itadmin/IPCashMarkUpConfig.js"></script>
}