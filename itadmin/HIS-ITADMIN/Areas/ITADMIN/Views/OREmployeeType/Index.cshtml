@{
    ViewBag.Title = "Index";
}


<input type="hidden" id="url"
data-getoremployeedashboard="@Url.Action("OREmployeeType")"
data-getemployeelist="@Url.Action("EmployeeList")"
data-vieworemployee="@Url.Action("SelectedListItem")"
data-viewempdetails="@Url.Action("OREmployeeViewDetails")"
data-save="@Url.Action("Save")"

/>
<div class="panel panel-default">
          <div class="panel-heading">
              <div class="row">
                 <div class="col-xs-3">
                    <h4><b class="navbar-header">OR Employee Type</b></h4>
                 </div>
                        <div class="form-group">
                       @* <label class="control-label col-sm-1">Type</label>*@
                @*        <div class="col-sm-2 pull-right">
                            <input type="text" id="select2PackageType" class="sel" />
                        </div>*@
                  @* <button id="btnNew" type="button" class="btn btn-sm btn-primary pull-right AR_custom_btn_fixed_width glyphicon glyphicon-folder-open ShowOnAdd ShowOnView HideOnEdit" data-dismiss="modal"> New</button>*@
              </div>
           </div>
        </div>
             <div class="panel-body">
                     <div class="col-xs-12">
                      <table id="gridItemList" class="table table-bordered table-hover dataTable no-footer;">
                         <thead class="AR_custom_table_header expanded-group">
                            <tr>
                               <th>Name</th>
                                <th>Type</th>
                               <th></th>
                            </tr>
                         </thead>
                      </table>
                          <button id="btnNew" type="button" class="btn btn-sm btn-primary pull-right AR_custom_btn_fixed_width glyphicon glyphicon-folder-open" data-dismiss="modal"> New</button>
             </div>

             </div>
      </div>
<div class="modal" id="modalEntry" tabindex="-1" style="overflow-y: auto;" role="dialog" aria-labelledby="myModalLabel">
   <input id="Registrationno" name="" type="hidden" class="form-control">
  
   <div class="modal-dialog modal-lg" style=" " role="document">
      <div class="modal-content">
         <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="opbmyModalLabel">OR Employee Type</h4>
         </div>
         <div class="modal-body">
            <div class="row">
               <div class="col-xs-12">
                  <form role="form" class="form-horizontal AR_custom_label_left_align">
                     <div class="form-group">
                             <label class="control-label col-sm-3">Employee:</label>
                        <div class="col-sm-8">
                           <input type="text" class="sel" id="select2Employee"/>
                        </div>
                     </div>
                     <div class="form-group">
                        <label class="control-label col-sm-3">Type:</label>
                         <div class="col-sm-4">
                           <input type="text" class="sel" id="select2Type"/>
                        </div>
                     @*    <label class="control-label col-sm-2">No of Visit:</label>
                        <div class="col-sm-2">
                           <input type="text" class="form-control" id="txtNoofVisit"/>
                        </div>*@
                     </div>
                  
                     @* 
                     <div id="endos"></div>
                     *@
                  </form>
               </div>
            </div>
            <div class="modal-footer">
                <button id="btnDelete" type="button" class="btn btn-sm btn-danger AR_custom_btn_fixed_width glyphicon glyphicon-trash HideOnAdd ShowOnEdit">Delete</button>
               <button id="btnSave" type="button" class="btn btn-sm btn-primary AR_custom_btn_fixed_width glyphicon glyphicon-floppy-disk ShowOnAdd ShowOnView HideOnEdit">Save</button>
               <button id="btnModify" type="button" class="btn btn-sm btn-info AR_custom_btn_fixed_width glyphicon glyphicon-pencil ShowOnEdit HideOnAdd">Modify</button>
               <button type="button" class="btn btn-sm btn-default AR_custom_btn_fixed_width" data-dismiss="modal">Cancel</button>
            </div>
         </div>
      </div>
   </div>
</div>
@section scripts{
<script src="~/Scripts/itadmin/OREmployeeType.js"></script>

}