@{
    ViewBag.Title = "Expired Policies";
}
<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="col-xs-5">
                <h4><span class="navbar-header">Expired Policies</span></h4>
            </div>
            <div class="navbar-collapse navbar-right">
                
                <label class="radio-inline">
                    <input type="radio" id="exp_new" name="exp" checked="checked" /> New
                </label>
                <label class="radio-inline">
                    <input type="radio" id="exp_terminated" name="exp" /> Terminated
                </label>
                <label class="radio-inline">
                    <input type="radio" id="exp_expired" name="exp" /> Expired
                </label>
                &nbsp;&nbsp;&nbsp;&nbsp;
                <button type="button" class="btn btn-sm btn-primary IT_custom_btn_fixed_width" id="reportpreview">
                    <span class="glyphicon glyphicon-eye-open"></span> Print Preview
                </button>
            </div>
        </div>
    </div>
    <div class="panel-body">
        <div class="form-group">
            <table class="table table-condensed table-bordered table-striped" id="dtdisplaytable">
                <thead class="IT_custom_table_header">
                    <tr>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                        <th></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>

    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="myModal" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" style="width:98% !important;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Report Generated</h4>
            </div>
            <div class="modal-body">
                <div id="report-container">
                    <iframe id="myReport2"></iframe>
                </div>
                <div class="clear-botn"></div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-sm btn-primary IT_custom_btn_fixed_width" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>


<input type="hidden" id="url"
       data-reporturl="@Url.Content("~/Areas/AROPBILLING/Reports/Reports.aspx")"
       data-arcommon="@Url.Content("~/Common")"
       data-getexppolicies="@Url.Action("get_expired_policies")"
/>
@Scripts.Render("~/Scripts/commonwrapper")
@Scripts.Render("~/Scripts/jsExpiredPolicies")